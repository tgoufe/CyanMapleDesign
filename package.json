{
  "name": "cyanmaple",
  "version": "2.1.4-0",
  "description": "A library based on lodash and vue 2.6+",
  "private": false,
  "main": "CMUI/lib/index.common.js",
  "scripts": {
    "start": "vue-cli-service serve --open",
    "precommit": "vue-cli-service lint",
    "lint:prettier": "cross-env prettier --write \"src/maple/**/*.js\" \"src/maple/**/*.vue\" --loglevel warn",
    "cyan": "gulp sass",
    "maple": "cross-env NODE_ENV=production && node ./build/rollup.maple && npm run theme",
    "theme": "gulp theme",
    "doc": "vuepress dev doc",
    "build": "cross-env NODE_ENV=production rimraf ./CMUI && node ./build/rollup.build && npm run cyan && npm run theme",
    "build:demo": "vue-cli-service build"
  },
  "files": [
    "src/",
    "CMUI/"
  ],
  "keywords": [
    "vue",
    "CyanMaple",
    "ui",
    "mobile"
  ],
  "author": "qqkillqq",
  "license": "MIT",
  "style": "CMUI/style/CMUI.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/tgoufe/CyanMapleDesign"
  },
  "homepage": "http://www.bingshangroup.com",
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-service": "^3.12.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "async-validator": "^3.2.3",
    "babel-eslint": "^10.0.1",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "faker": "^4.1.0",
    "gulp": "^4.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^3.0.9",
    "inquirer": "^7.0.0",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-simple-vars": "^5.0.2",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.23.1",
    "rollup-plugin-alias": "~2.2.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-vue": "^5.0.1",
    "sass-loader": "^7.3.1",
    "tasksfile": "^5.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "async-validator": "^3.2.2",
    "core-js": "^2.6.5",
    "dayjs": "^1.8.16",
    "lodash": "^4.17.15",
    "svg-sprite-loader": "^4.1.6",
    "swiper": "^5.0.4",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "iOS >= 8",
    "Android >= 4"
  ]
}
